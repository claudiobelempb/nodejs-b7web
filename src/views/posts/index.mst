{{>header/index}}
<main>
<h1>Posts 2</h1>

{{^posts}}
  <div>Not Posts</div>
{{/posts}}
{{#posts}}
<div>
  <a href="posts/{{slug}}"><h2>{{title}}</h2></a>
  <p>{{body}}</p>
  <div>
    {{#tags}}
      <small>{{.}}</small>
    {{/tags}}
  </div>
  <br>
  <a href="posts/{{slug}}/edit">[ Edit ]</a>
  <hr>
</div>
{{/posts}}
</main>